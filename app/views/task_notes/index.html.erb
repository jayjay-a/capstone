<p id="notice"><%= notice %></p>

<h1>Task Notes</h1>

<table>
  <thead>
    <tr>
      <th>Task</th>
      <th>Task notes</th>
      <th>Task note owner</th>
      <th>Taske note date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @task_notes.each do |task_note| %>
      <tr>
        <td><%= task_note.task_id %></td>
        <td><%= task_note.task_notes %></td>
        <td><%= task_note.task_note_owner %></td>
        <td><%= task_note.taske_note_date %></td>
        <td><%= link_to 'Show', task_note %></td>
        <td><%= link_to 'Edit', edit_task_note_path(task_note) %></td>
        <td><%= link_to 'Destroy', task_note, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Task Note', new_task_note_path %>

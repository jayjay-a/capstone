<h1>Report28</h1>
<p id="notice"><%= notice %></p>

<h2>Show Project Notes</h2>

<table class="table">
  <thead>
  <tr>
    <th>ID</th>
    <th>Customer Name</th>
    <th>Customer Branch</th>
    <th>Project Type Description</th>
    <th>Project Status Description</th>
    <th>Project Start Date</th>
    <th>Project End Date</th>
    <th>Project Notes</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @proj_ntes.each do |notes| %>
      <tr>
        <td><%= notes['project_id'] %></td>
        <td><%= notes['customer_name'] %></td>
        <td><%= notes['customer_branch'] %></td>
        <td><%= notes['project_type_description'] %></td>
        <td><%= notes['project_status_description'] %></td>
        <td><%= notes['project_start_date'] %></td>
        <td><%= notes['project_end_date'] %></td>
        <td><%= notes['project_notes'] %></td>
      </tr>
  <% end %>
  </tbody>
</table>

<br>



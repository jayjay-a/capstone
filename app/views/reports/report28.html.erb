<!--<h1>Report28</h1>-->
<p id="notice"><%= notice %></p>

<h2>Project Notes</h2>
<h4>for Waiting Bids, Accepted Bids, and Active Projects</h4>
<h6>Download:
  <%= link_to "Excel", reports_report28_path(format: "xlsx") %> |
  <%= link_to "PDF", reports_report28_path(format: "pdf") %>
</h6>

<table class="table">
  <thead>
  <tr>
    <th>Project ID</th>
    <th>Customer Name</th>
    <th>Customer Branch</th>
    <th>Project Type</th>
    <th>Project Status</th>
    <th>Bid Submit Date</th>
    <th>Project Start Date</th>
    <th>Project Notes</th>
    <th colspan="8"></th>
  </tr>
  </thead>

  <tbody>
  <% @proj_notes.each do |notes| %>
      <tr>
        <td><%= notes['project_id'] %></td>
        <td><%= notes['customer_name'] %></td>
        <td><%= notes['customer_branch'] %></td>
        <td><%= notes['project_type_description'] %></td>
        <td><%= notes['project_status_description'] %></td>
        <td><%= notes['bid_submit_date'] %></td>
        <td><%= notes['project_start_date'] %></td>
        <td><%= notes['project_notes'] %></td>
      </tr>
  <% end %>
  </tbody>
</table>

<br>



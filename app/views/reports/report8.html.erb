<h1>Report8</h1>
<p id="notice"><%= notice %></p>

<h2>Show Awaiting Bids</h2>

<table class="table">
  <thead>
  <tr>
    <th>ID</th>
    <th>Customer Name</th>
    <th>Customer Branch</th>
    <th>Material Description</th>
    <th>Quantity</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @specific_matlist.each do |matlist| %>
      <tr>
        <td><%= matlist['project_id'] %></td>
        <td><%= matlist['customer_name'] %></td>
        <td><%= matlist['customer_branch'] %></td>
        <td><%= matlist['material_description'] %></td>
        <td><%= matlist['quantity'] %></td>
      </tr>
  <% end %>
  </tbody>
</table>

<br>

<!--<h1>Report14</h1>-->
<p id="notice"><%= notice %></p>

<h2>Project Duration</h2>
<h6>
  Download:
  <%= link_to "Excel", reports_project_duration_path(format: "xlsx") %> |
  <%= link_to "PDF", reports_project_duration_path(format: "pdf") %>
</h6>

<table class="responsive-table">
  <thead>
  <tr>
    <th>Project ID</th>
    <th>Customer Name</th>
    <th>Customer Branch</th>
    <th>Project Type</th>
    <th>Project Start Date</th>
    <th>Project End Date</th>
    <th># of Days</th>
  </tr>
  </thead>

  <tbody>
  <% @duration_proj.each do |duration| %>
      <tr>
        <td><%= duration['project_id'] %></td>
        <td><%= duration['customer_name'] %></td>
        <td><%= duration['customer_branch'] %></td>
        <td><%= duration['project_type_description'] %></td>
        <td><%= duration['project_start_date'] %></td>
        <td><%= duration['project_end_date'] %></td>
        <td><%= duration['days'] %></td>
      </tr>
  <% end %>
  </tbody>
</table>

<br>
wb = xlsx_package.workbook

wb.add_worksheet(name: "Term_Subs") do |sheet|
  # this is the head row of your spreadsheet
  sheet.add_row %w(subcontractor_name)
  
  # each user is a row on your spreadsheet
  @term_subs.each do |subcontractor|
    sheet.add_row [subcontractor['subcontractor_name']]
  end
end
wb = xlsx_package.workbook

wb.add_worksheet(name: "Terminated Subcontractors") do |sheet|
  # this is the head row of your spreadsheet
  sheet.add_row %w(Subcontractor_Name Company Phone Email)
  
  # each user is a row on your spreadsheet
  @term_subs.each do |subcontractor|
    sheet.add_row [subcontractor['subcontractor_name'], subcontractor['company'], subcontractor['subcontractor_phone'],
    subcontractor['subcontractor_email']]
  end
end
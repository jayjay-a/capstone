<h1>Report21</h1>
<p id="notice"><%= notice %></p>

<h2>Show Projects for this month</h2>
<table class="table">
  <thead>
  <tr>
    <th>Project ID</th>
    <th>Customer ID</th>
    <th>Project Type ID</th>
    <th>Project Status ID</th>
    <th>Project Start Date</th>
    <th>Project End Date</th>
    <th>Bid Submit Date</th>
    <th>Bid Amount</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @weekly_proj.each do |a| %>
      <tr>
        <td><%= a['project_id'] %></td>
        <td><%= a['customer_id'] %></td>
        <td><%= a['project_type_id'] %></td>
        <td><%= a['project_status_id'] %></td>
        <td><%= a['project_start_date'] %></td>
        <td><%= a['project_end_date'] %></td>
        <td><%= a['bid_submit_date'] %></td>
        <td><%= a['bid_amount'] %></td>
      </tr>
  <% end %>
  </tbody>
</table>

<br>

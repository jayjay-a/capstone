  <thead>
  <tr>
    <th>Project ID</th>
    <th>Customer Name</th>
    <th>Customer Branch</th>
    <!-- <th>Job Type Description</th> -->
    <th>Days Duration</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @duration_proj.each do |duration| %>
      <tr>
        <td><%= duration['project_id'] %></td>
        <td><%= duration['customer_name'] %></td>
        <td><%= duration['customer_branch'] %></td>
        <!--<td><!-- %= duration['job_type_description'] %></td> -->
        <td><%= duration['days'] %></td>
      </tr>
  <% end %>
  </tbody>
</table>


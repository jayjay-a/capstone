<h1>Report20</h1>
<p id="notice"><%= notice %></p>

<h2>Show Rental List for This Project</h2>
<table class="table">
  <thead>
  <tr>
    <th>Project ID</th>
    <th>Rental List ID</th>
    <th>Rental Price</th>
    <th>Cost Frequency</th>
    <th>Rental Equipment ID/th>
    <th>Rental Equipment Description</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @ongoing_jobs.each do |a| %>
      <tr>
        <td><%= a['project_id'] %></td>
        <td><%= a['rental_list_id'] %></td>
        <td><%= a['rental_price'] %></td>
        <td><%= a['cost_frequency'] %></td>
        <td><%= a['rental_equipment_id'] %></td>
        <td><%= a['rental_description'] %></td>
      </tr>
  <% end %>
  </tbody>
</table>

<br>

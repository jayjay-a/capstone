<h1>Report13</h1>
<p id="notice"><%= notice %></p>

<h2>Show All Projects of a certain Project Type</h2>

<table class="table">
  <thead>
  <tr>
    <th>Project ID</th>
    <th>Project Type Description</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @specific_projtype.each do |projlist| %>
      <tr>
        <td><%= projlist['project_id'] %></td>
        <td><%= projlist['project_type_description'] %></td>
      </tr>
  <% end %>
  </tbody>
</table>

<br>

<h1>Report10</h1>
<p id="notice"><%= notice %></p>

<h2>Projects and their Jobs</h2>
<h6>Download:
  <%= link_to "Excel", reports_report10_path(format: "xlsx") %> |
  <%= link_to "PDF", reports_report10_path(format: "pdf") %></h6>

<table class="table">
  <thead>
  <tr>
    <th>Project ID</th>
    <th>Customer Name</th>
    <th>Customer Branch</th>
    <th>Job Type Description</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @specific_jobproj.each do |projlist| %>
      <tr>
        <td><%= projlist['project_id'] %></td>
        <td><%= projlist['customer_name'] %></td>
        <td><%= projlist['customer_branch'] %></td>
        <td><%= projlist['job_type_description'] %></td>
      </tr>
  <% end %>
  </tbody>
</table>

<br>

<h2>Show Projects with a certain job type</h2>

<table class="table">
  <thead>
  <tr>
    <th>Project ID</th>
    <th>Job Type Description</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @specific_jobproj.each do |projlist| %>
      <tr>
        <td><%= projlist['project_id'] %></td>
        <td><%= projlist['job_type_description'] %></td>
      </tr>
  <% end %>
  </tbody>
</table>


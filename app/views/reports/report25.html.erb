<h1>Report25</h1>
<p id="notice"><%= notice %></p>

<h2>Show Projects for this year</h2>


<%= form_tag reports_report2_path, method: :get do %>
    <h6>Download:
      <%= link_to "Excel", reports_report25_path(format: "xlsx", :q => params[:q], :t => params[:t]) %> |
      <%= link_to "PDF", reports_report25_path(format: "pdf",  :q => params[:q], :t => params[:t]) %>
    </h6>

      <div class="field col s2">
        <%= text_field_tag :f, id: :project_project_start_date, placeholder: "YYYY-MM-DD", :class => "datepicker" %>
      </div>

<% end %>

<table class="table">
  <thead>
  <tr>
    <th>Project ID</th>
    <th>Customer Name</th>
    <th>Customer Branch</th>
    <th>Project Type</th>
    <th>Project Status</th>
    <th>Project Start Date</th>
    <th>Project End Date</th>
    <th>Bid Amount</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @yearly_proj.each do |a| %>
      <tr>
        <td><%= a['project_id'] %></td>
        <td><%= a['customer_name'] %></td>
        <td><%= a['customer_branch'] %></td>
        <td><%= a['project_type_description'] %></td>
        <td><%= a['project_status_description'] %></td>
        <td><%= a['project_start_date'] %></td>
        <td><%= a['project_end_date'] %></td>
        <td><%= a['bid_amount'] %></td>
      </tr>
  <% end %>
  </tbody>
</table>

<br>

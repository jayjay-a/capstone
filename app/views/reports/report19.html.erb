<h1>Report19</h1>
<p id="notice"><%= notice %></p>

<h2>Show Ongoing Projects</h2>
<table class="table">
  <thead>
  <tr>
    <th>Project ID</th>
    <th>Job ID</th>
    <th>Job Status ID</th>
    <th>Job Status Description</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @ongoing_jobs.each do |complete| %>
      <tr>
        <td><%= complete['project_id'] %></td>
        <td><%= complete['job_id'] %></td>
        <td><%= complete['job_status_id'] %></td>
        <td><%= complete['job_status_description'] %></td>
      </tr>
  <% end %>
  </tbody>
</table>

<br>


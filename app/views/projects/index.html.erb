<p id="notice"><%= notice %></p>

<h1>Projects</h1>

<table>
  <thead>
    <tr>
      <th>Customer</th>
      <th>Project status</th>
      <th>Er</th>
      <th>Project type</th>
      <th>Project start date</th>
      <th>Project end date</th>
      <th>Bid submit date</th>
      <th>Bid material cost</th>
      <th>Bid cost of labor decimal</th>
      <th>Bid cost of permits</th>
      <th>Bid equipment rental</th>
      <th>Bid freight decimal</th>
      <th>Tax rate</th>
      <th>Bid amount</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @projects.each do |project| %>
      <tr>
        <td><%= project.customer_id %></td>
        <td><%= project.project_status_id %></td>
        <td><%= project.er %></td>
        <td><%= project.project_type_id %></td>
        <td><%= project.project_start_date %></td>
        <td><%= project.project_end_date %></td>
        <td><%= project.bid_submit_date %></td>
        <td><%= project.bid_material_cost %></td>
        <td><%= project.bid_cost_of_labor_decimal %></td>
        <td><%= project.bid_cost_of_permits %></td>
        <td><%= project.bid_equipment_rental %></td>
        <td><%= project.bid_freight_decimal %></td>
        <td><%= project.tax_rate %></td>
        <td><%= project.bid_amount %></td>
        <td><%= link_to 'Show', project %></td>
        <td><%= link_to 'Edit', edit_project_path(project) %></td>
        <td><%= link_to 'Destroy', project, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Project', new_project_path %>
